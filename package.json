{
  "name": "daily-api",
  "version": "0.2.0",
  "description": "API for Spark Daily",
  "main": "server.js",
  "engines": {
    "node": "6.3.0",
    "npm": "3.10.5"
  },
  "scripts": {
    "start": "node server.js",
    "pretest": "npm run lint",
    "test": "NODE_ENV=test PORT=3001 DATABASE_URL=postgresql://localhost:5432/test",
    "lint": "semistandard --fix --verbose | snazzy",
    "debug": "DEBUG=1 nodemon server.js",
    "pg-config": "node database/config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sparkthecause/daily-api.git"
  },
  "author": "Charles Burgess",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sparkthecause/daily-api/issues"
  },
  "homepage": "https://github.com/sparkthecause/daily-api",
  "dependencies": {
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "cors": "^2.7.1",
    "cron": "^1.1.0",
    "daily-templates": "^0.4.0",
    "express": "^4.14.0",
    "express-sslify": "^1.1.0",
    "inline-css": "^2.2.2",
    "knex": "^0.11.7",
    "moment": "^2.14.1",
    "moment-business": "^3.0.0",
    "pg": "^6.0.2",
    "postmark": "^1.2.1",
    "validator": "^5.4.0"
  },
  "devDependencies": {
    "dotenv": "^2.0.0",
    "nodemon": "^1.9.2",
    "semistandard": "^8.0.0",
    "snazzy": "^4.0.0"
  }
