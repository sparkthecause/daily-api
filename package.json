  },
  "homepage": "https://github.com/sparkthecause/daily#readme",
  "dependencies": {
    "bluebird": "^3.2.1",
    "express": "^4.13.3",
    "express-handlebars": "^2.0.1",
    "express-sslify": "^1.0.1",
    "knex": "^0.9.0",
    "pg": "^4.4.4"
  },
  "devDependencies": {
    "pg-monitor": "^0.4.5",
    "piping": "^0.3.0"
  }
